                        | {% trans ' Settings' %}
                li
                    a(href="{% url 'mgmt:help' %}")
                        i.fa.fa-question
                        | {% trans ' Help' %}
                li
                    a(href="/")
                        i.fa.fa-sign-out
                        | {% trans ' Sign Out' %}


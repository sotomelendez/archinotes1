    script(type="text/javascript")
        $(document).ready(function() {
            var prev = document.referrer;
            if(prev.indexOf("context") != -1)
            {
                $('#collapseOne').addClass("in");
            }
            else if(prev.indexOf("functional") != -1)
            {
                $('#collapseTwo').addClass("in");
            }
            else if(prev.indexOf("deployment") != -1)
            {
                $('#collapseThree').addClass("in");
            }
            else if(prev.indexOf("information") != -1)
            {
                $('#collapseFour').addClass("in");
            }
            else if(prev.indexOf("concurrency") != -1)
            {
                $('#collapseFive').addClass("in");
            }
            else if(prev.indexOf("development") != -1)
            {
                $('#collapseSix').addClass("in");
            }
            $.repeat().add('connection').each($).connections('update').wait(0);
        });
